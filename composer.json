{
    "name": "jflepp/picomailformsplugin",
    "description": "A mail plugin with forms for pico",
    "type": "pico-plugin",
    "require": {
        "phpmailer/phpmailer": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": { 
            "PicoMailPlugin\\": "src/",
            "PicoMailPlugin\\Test\\": "tests/"
        },
	"classmap": ["PicoMailFormsPlugin.php"]
    },
    "extra": {
        "installer-name": "PicoMailFormsPlugin"
    }
}
